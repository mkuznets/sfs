{
  "dev": {
    "base_url": "http://127.0.0.1:8080",
    "Security": {
      "Auth": {
        "oauth": {
          "Type": "OAuth2",
          "Grant Type": "Client Credentials",
          "Client ID": "{{client_id}}",
          "Client Secret": "{{client_secret}}",
          "Token URL": "{{token_url}}",
          "Custom Request Parameters": {
            "audience": {
              "Value": "https://sfs.mkuznets.com",
              "Use": "In Token Request"
            }
          }
        }
      }
    }
  },
  "prod": {
    "base_url": "https://sfs.mkuznets.com",
    "Security": {
      "Auth": {
        "oauth": {
          "Type": "OAuth2",
          "Grant Type": "Client Credentials",
          "Client ID": "{{client_id}}",
          "Client Secret": "{{client_secret}}",
          "Token URL": "{{token_url}}",
          "Custom Request Parameters": {
            "audience": {
              "Value": "https://sfs.mkuznets.com",
              "Use": "In Token Request"
            }
          }
        }
      }
    }
  }
}
